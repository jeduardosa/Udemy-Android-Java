<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loginproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_my_app"
        android:label="Login Project"
        android:roundIcon="@mipmap/ic_my_app"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.Lista2Activity" android:parentActivityName=".views.PrincipalActivity" />

        <activity android:name=".views.Lista1Activity" android:parentActivityName=".views.PrincipalActivity" />

        <activity android:name=".views.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.CadastroActivity" />
        <activity android:name=".views.PrincipalActivity" />
        <activity android:name=".views.MainActivity" />
    </application>

</manifest>